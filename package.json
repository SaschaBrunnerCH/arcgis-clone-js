{
  "name": "arcgis-clone-js",
  "version": "0.4.0",
  "description": "Transfer sets of ArcGIS Online items from one organization to another",
  "main": "dist/node/index.js",
  "browser": "dist/umd/arcgis-clone.umd.js",
  "module": "dist/esm/index.js",
  "js:next": "dist/esm/index.js",
  "scripts": {
    "build": "npm run build:node && npm run build:umd && npm run build:esm",
    "build:esm": "tsc --module es2015 --outDir ./dist/esm --declaration",
    "build:umd": "rollup -c ./umd-base-profile.js && rollup -c ./umd-production-profile.js",
    "build:node": "tsc --module commonjs --outDir ./dist/node",
    "build:tsumd": "tsc --module umd --outDir ./dist/tsumd",
    "dev:esm": "tsc -w --module es2015 --outDir ./dist/esm --declaration",
    "dev:umd": "rollup -w -c ./umd-base-profile.js",
    "dev:node": "tsc -w --module commonjs --outDir ./dist/node",
    "lint": "tslint --project tsconfig.json",
    "test": "npm run test:node && npm run test:chrome",
    "test:ci": "npm run lint && npm run test:node && npm run test:chrome:ci && npm run test:firefox",
    "test:chrome": "karma start --single-run --browsers=ChromeHeadlessCI",
    "test:chrome:ci": "karma start --single-run --browsers ChromeHeadlessCI karma.conf.js",
    "test:firefox": "karma start --single-run --browsers=Firefox",
    "test:node": "ts-node node_modules/jasmine/bin/jasmine --config=jasmine.json",
    "docs": "typedoc --target ES5 --out ./docs/src/ src/index.ts",
    "ver": "tsc -v",
    "docs:mocks": "typedoc --target ES5 --out ./docs/mocks/ test/mocks/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Esri/arcgis-clone-js.git"
  },
  "keywords": [],
  "author": "Esri",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/Esri/arcgis-clone-js/issues"
  },
  "homepage": "https://github.com/Esri/arcgis-clone-js#readme",
  "devDependencies": {
    "@esri/arcgis-rest-auth": "^1.14.3",
    "@esri/arcgis-rest-common-types": "^1.14.3",
    "@esri/arcgis-rest-feature-service-admin": "^1.14.3",
    "@esri/arcgis-rest-groups": "^1.14.3",
    "@esri/arcgis-rest-items": "^1.14.3",
    "@esri/arcgis-rest-request": "^1.14.3",
    "@esri/arcgis-rest-sharing": "^1.14.3",
    "@types/arcgis-js-api": "^4.9.0",
    "@types/fetch-mock": "^6.0.4",
    "@types/isomorphic-fetch": "0.0.34",
    "@types/jasmine": "^2.8.9",
    "@types/node": "^10.11.7",
    "coveralls": "^3.0.2",
    "fetch-mock": "^7.0.7",
    "jasmine": "^3.2.0",
    "jasmine-core": "^3.2.1",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^3.1.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coveralls": "^2.1.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "^1.1.2",
    "karma-requirejs": "^1.1.0",
    "karma-typescript": "^3.0.13",
    "karma-typescript-es6-transform": "^1.0.4",
    "node-fetch": "^2.2.0",
    "requirejs": "^2.3.6",
    "rollup": "^0.66.4",
    "rollup-plugin-commonjs": "^8.2.6",
    "rollup-plugin-filesize": "^5.0.1",
    "rollup-plugin-json": "^2.3.0",
    "rollup-plugin-node-resolve": "^3.0.3",
    "rollup-plugin-typescript2": "^0.18.0",
    "rollup-plugin-uglify": "^3.0.0",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.6.0",
    "tslint-config-standard": "^6.0.1",
    "typedoc": "^0.13.0",
    "typescript": "^3.1.2"
  },
  "dependencies": {
    "es6-promise": "^4.2.5",
    "isomorphic-fetch": "^2.2.1",
    "isomorphic-form-data": "^2.0.0",
    "tslib": "^1.9.3"
  }
}
